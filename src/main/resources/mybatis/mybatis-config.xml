<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD SQL Map Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="callSettersOnNulls" value="true"/>
        <setting name="jdbcTypeForNull" value="NULL"/>
        <setting name="cacheEnabled" value="false"/>
    </settings>

    <typeAliases>
        <typeAlias type="com.gozin.mainboard.member.dto.MemberDTO" alias="MemberDTO"/>
        <typeAlias type="com.gozin.mainboard.coupon.dto.CouponDTO" alias="CouponDTO"/>
        <typeAlias type="com.gozin.mainboard.coupon.dto.SearchCouponDTO" alias="SearchCouponDTO"/>
        <typeAlias type="com.gozin.mainboard.payment.dto.PaymentDTO" alias="PaymentDTO"/>
        <typeAlias type="com.gozin.mainboard.order.dto.OrderDTO" alias="OrderDTO"/>
        <typeAlias type="com.gozin.mainboard.delivery.model.dto.DeliveryDTO" alias="DeliveryDTO"/>
        <typeAlias type="com.gozin.mainboard.delivery.model.dto.DeliveryAddressDTO" alias="DeliveryAddressDTO"/>
    </typeAliases>

    <mappers>
        <mapper resource="MemberMapper.xml"/>
        <mapper resource="CouponMapper.xml"/>
        <mapper resource="PaymentMapper.xml"/>
        <mapper resource="OrderMapper.xml"/>
        <mapper resource="DeliveryMapper.xml"/>
        <mapper resource="DeliveryAddressMapper.xml"/>
    </mappers>
</configuration>